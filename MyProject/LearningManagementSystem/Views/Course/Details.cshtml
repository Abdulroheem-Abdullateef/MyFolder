@model LearningManagementSystem.DTOs.CourseDto

@{
    Layout = "_InstructorLayout";
    ViewData["Title"] = "Instructor";
}


<dl class="row">
    <dt class="col-sm-2">
        Name:
    </dt>
    <dd class="col-sm-10">
        @Model.CourseName
    </dd>

    <dt class="col-sm-2">
        CourseCode:
    </dt>
    <dd class="col-sm-10">
        @Model.CourseCode
    </dd>
    <dt class="col-sm-2">
        InstructorName:
    </dt>
    <dd class="col-sm-10">
        @Model.InstructorName
    </dd>
    <dt class="col-sm-2">
        StudentName:
    </dt>
    <dd class="col-sm-10">
        @Model.Students
    </dd>



</dl>

<button style="background-color:darkblue;"><a asp-action="Index">Back to List</a></button>