@model WebApplicationMVC.DTO.CityDto


<h3>Are you sure you want to delete this City?</h3>
<div style="font-size: 130%;">
    <hr />
    <dl class="row">

        <dt class="col-sm-2">
            Code:
        </dt>
        <dd class="col-sm-10">
            @Model.Id
        </dd>


        <dt class="col-sm-2">
            CityCode:
        </dt>
        <dd class="col-sm-10">
            @Model.CityCode
        </dd>

        <dt class="col-sm-2">
            CityName:
        </dt>
        <dd class="col-sm-10">
            @Model.CityName
        </dd>


        <dt class="col-sm-2">
            Population:
        </dt>
        <dd class="col-sm-10">
            @Model.Population
        </dd>

    </dl>

    <form asp-action="Delete">
        <input type="submit" value="Delete" class="btn btn-danger" /> |
        <a asp-action="Index">Back to List</a>
    </form>
</div>
