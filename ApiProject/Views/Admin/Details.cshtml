@{
   Layout = "AdminLayout";
}
@model ApiProject.Dtos.AdminDTO


<dl class="row">
     <dt class="col-sm-2">
        Photo:
    </dt>
    <dd class="col-sm-10">
         <img class="card-img" style="height:200px; width:200px" src="~/AdminPhotos/@Model.AdminPhoto"/>
        <a href="~/AdminImages/@Model.AdminPhoto" download="@Model.AdminPhoto">
        <input class="btn btn-outline-primary" type="button" value="Download Photo" /></a>
    </dd>
    <dt class="col-sm-2">
        FirstName:
    </dt>
    <dd class="col-sm-10">
        @Model.FirstName
    </dd>
    <dt class="col-sm-2">
        LastName:
    </dt>
    <dd class="col-sm-10">
        @Model.LastName
    </dd>
     <dt class="col-sm-2">
        Email:
    </dt>
    <dd class="col-sm-10">
        @Model.Email
    </dd>
     <dt class="col-sm-2">
        Gender:
    </dt>
    <dd class="col-sm-10">
        @Model.Gender
    </dd>
     <dt class="col-sm-2">
        Address:
    </dt>
    <dd class="col-sm-10">
        @Model.Address
    </dd>
    
    
</dl>